{
	"name": "tabulate/tabulate",
	"description": "WordPress plugin for managing relational tabular data.",
	"license": "GPL-2.0+",
	"autoload": {
		"psr-4": {
			"WordPress\\Tabulate\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [ "tests/TestBase.php" ]
	},
	"config": {
		"platform": {
			"php": "5.6"
		}
	},
	"require": {
		"php": "^5.6|^7.0",
		"twig/twig": "^1.0",
		"yohang/calendr": "^1.0",
		"phayes/geophp": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.0",
		"wp-coding-standards/wpcs": "^0.11"
	}
}
